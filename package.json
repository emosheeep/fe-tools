{
  "name": "vite-plugin-lib-inject-css",
  "type": "module",
  "version": "2.2.0",
  "packageManager": "pnpm@9.15.1",
  "description": "Inject css at the top of chunk file in lib mode using `import` statement, support multiple entries.",
  "author": "秦旭洋 <emosheep@qq.com>",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/emosheeep/vite-plugin-lib-inject-css"
  },
  "keywords": [
    "vite",
    "plugin",
    "lib",
    "build",
    "vite-lib",
    "vite-plugin",
    "vite-build",
    "css",
    "style",
    "styles",
    "inject",
    "inline"
  ],
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js",
      "require": "./dist/index.cjs"
    }
  },
  "main": "./dist/index.cjs",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "files": [
    "CHANGELOG.md",
    "dist"
  ],
  "scripts": {
    "prepare": "simple-git-hooks",
    "prepublishOnly": "npm run build",
    "watch": "tsup --watch",
    "build": "tsup",
    "lint": "eslint . --fix",
    "changeset": "changeset",
    "versions": "changeset version"
  },
  "peerDependencies": {
    "vite": "*"
  },
  "dependencies": {
    "@ast-grep/napi": "^0.32.2",
    "magic-string": "^0.30.17",
    "picocolors": "^1.1.1"
  },
  "devDependencies": {
    "@antfu/eslint-config": "^3.12.1",
    "@changesets/cli": "^2.27.11",
    "@commitlint/cli": "^19.6.1",
    "@commitlint/config-conventional": "^19.6.0",
    "@types/node": "^20.17.10",
    "eslint": "^9.17.0",
    "simple-git-hooks": "^2.11.1",
    "tsup": "^8.3.5",
    "typescript": "^5.7.2",
    "vite": "^6.0.6"
  },
  "simple-git-hooks": {
    "commit-msg": "npx commitlint -e",
    "pre-commit": "npx eslint . && npx tsc --noEmit"
  }
}
